last minute steps to make an e2msa tarball:
(1) copy the userguide
(2) distclean
(3) make tar and zip
-----------------------------------------------------------------------------------------------------------------
for v in e2msa_v0.1 ; do \
for x in 0 ; do \
cp ~/src/src/mysource/documentation/e2msa/e2msa_userguide.pdf ~/src/src/mysource/versions/e2msa/$v/documentation/. ;\
cd ~/src/src/mysource/versions/e2msa/$v ;\
make distclean ;\
cd .. ;\
cp -rf $v $v.$x ;\
cd $v.$x ;\
rm -rf lib/easel/.git ;\
cd .. ;\
rm $v.$x.tar.* ;\
tar cvf $v.$x.tar $v.$x ;\
gzip $v.$x.tar ;\
rm -rf $v.$x ;\
done
done

