@article{batzer:alu,
  author={Batzer, MA and Deininger, PL},
  title={Alu repeats and human genomic diversity},
  journal=nrg,
  volume=3,
  number=5,
  month=may,
  year=2002,
  pages={370-379},
  abstract={
	During the past 65 million years, Alu elements have propagated
	to more than one million copies in primate genomes, which has
	resulted in the generation of a series of Alu subfamilies of
	different ages. Alu elements affect the genome in several ways,
	causing insertion mutations, recombination between elements,
	gene conversion and alterations in gene expression. Alu-insertion
	polymorphisms are a boon for the study of human population genetics
	and primate comparative genomics because they are neutral genetic
	markers of identical descent with known ancestral states.}}

