@article{mandal:gcvT,
  author={Mandal, Maumita and Lee, Mark and Barrick, Jeffrey E. and 
	Weinberg, Zasha and Emilsson, Gail Mitchell and Ruzzo, Walter L. and Breaker, Ronald R.},
  title={A Glycine-dependent Riboswitch that Uses
	Cooperative Binding to Control Gene Expression in Bacteria},
  journal=science,
  year=2004,
  month="8 " # oct,
  volume=306,
  pages={275-279},
  annote={Corrected figure, 26 Nov.},
  abstract={A new riboswitch class that is selectively triggered by
	glycine has been identified in bacteria. A representative of these
	glycine-sensing RNAs from Bacillus subtilis operates as a rare
	genetic ON switch for the gcvT operon which codes for proteins that form
	the glycine cleavage system. In most instances, glycine riboswitches
	integrate two ligand-binding domains that function cooperatively to
	more closely approximate a two-state genetic switch. This advanced
	form of riboswitch may have evolved to ensure that excess glycine
	is efficiently used to provide carbon flux through the citric
	acid cycle while maintaining adequate levels of the amino acid for
	protein synthesis. Thus, riboswitches perform key regulatory roles
	and exhibit complex performance characteristics that previously had
	been observed only with protein factors.}}

